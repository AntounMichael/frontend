{{programyear-competencies programYear=programYear}}
{{#liquid-if (or (eq programYear.objectives.length 0) pyObjectiveDetails)}}
  {{detail-objectives
  subject=programYear
  isProgramYear=true
  editable=editable
  collapse=(action this.attrs.setPyObjectiveDetails false)
  expand=(action this.attrs.setPyObjectiveDetails true)
  }}
{{else}}
  {{collapsed-objectives subject=programYear editable=editable expand=(action this.attrs.setPyObjectiveDetails true)}}
{{/liquid-if}}
{{detail-stewards programYear=programYear}}
