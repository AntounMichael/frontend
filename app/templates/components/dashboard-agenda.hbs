<table>
  <tbody>
    {{#each weeksEvents as |event|}}
      {{#link-to 'events' event.slug}}
        <tr class='clickable'>
          <td class='event-date'>{{fa-icon 'external-link-square'}} {{moment event.date}}</td>
          <td>{{event.name}}</td>
          <td>{{event.location}}</td>
        </tr>
      {{/link-to}}
    {{/each}}
  </tbody>
</table>
{{events}}
