<div class="backtolink">{{#link-to 'courses'}}{{t 'courses.backToCourses'}}{{/link-to}}</div>

<section id='course-details' class='full-width course-details'>
  {{ilios-course-details
    course=model
    collapsed=collapsed
    collapsedState='collapsedState'
    courseObjectiveDetails=courseObjectiveDetails
    setCourseObjectiveDetails=(action (mut courseObjectiveDetails))
    setTaxonomyDetails=(action (mut taxonomyDetails))
  }}
  {{liquid-outlet}}
</section>
