<div class="backtolink backtosessionlink">{{#link-to 'course' courseController.model}}{{t 'general.backToSessionList'}}{{/link-to}}</div>
{{session-details
  session=model
  course=courseController.model
  programs=courseController.programs
  sessionTypes=sessionController.sessionTypes
  sessionObjectiveDetails=sessionObjectiveDetails
  sessionTaxonomyDetails=sessionTaxonomyDetails
  setSessionObjectiveDetails=(action (mut sessionObjectiveDetails))
  setSessionTaxonomyDetails=(action (mut sessionTaxonomyDetails))
  isManagingLearnerGroups=isManagingLearnerGroups
  setIsManagingLearnerGroups=(action (mut isManagingLearnerGroups))
  sessionLearnergroupDetails=sessionLearnergroupDetails
  setSessionLearnergroupDetails=(action (mut sessionLearnergroupDetails))
  showNewOfferingForm=showNewOfferingForm
  toggleShowNewOfferingForm=(toggle-action 'showNewOfferingForm' this)
}}
